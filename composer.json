{
    "name": "rondarby/laracomm",
    "version": "dev-master",
    "source": {
        "url": "https://github.com/RonDarby/laracomm",
        "type": "git",
        "reference": "master"
    },
    "description": "Ecommerce Package for Laravel",
    "keywords": [
        "ecommerce",
        "laracomm",
        "laravel"
    ],
    "homepage": "https://github.com/RonDarby/laracomm",
    "license": "LGPL-3",
    "authors": [
        {
            "name": "Ron Darby",
            "email": "ron@cozareg.co.za",
            "homepage": "http://cozareg.co.za",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.5.9",
        "cartalyst/support": "1.0.*"
    },
    "repositories": [
        {
          "type": "composer",
          "url": "https://packages.cartalyst.com"
        }
      ],
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Rondarby\\Laracomm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rondarby\\Laracomm\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.7-dev"
        }
    }
}
