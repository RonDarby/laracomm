{
    "name": "rondarby/laracomm",
    "description": ":package_description",
    "keywords": [
        "league",
        ":package_name"
    ],
    "homepage": "https://github.com/thephprondarby/laracomm",
    "license": "MIT",
    "authors": [
        {
            "name": "Ron Darby",
            "email": "ron@cozareg.co.za",
            "homepage": "http://cozareg.co.za",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0",
        "cartalyst/themes": "3.0.*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.cartalyst.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Rondarby\\Laracomm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rondarby\\Laracomm\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
      "github-oauth": {
        "github.com": "0c4140926f98b9c640ed882b15d372b1719becc3"
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
